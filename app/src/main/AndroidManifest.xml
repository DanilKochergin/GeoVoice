        android:label="@string/app_name"
        android:roundIcon="@mipmap/geovoice_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GeoVoice">
        <activity
            android:name=".places.CongradulationsActivity"
            android:exported="false" />
        <activity
